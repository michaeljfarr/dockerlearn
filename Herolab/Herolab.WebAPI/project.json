{
    /* Click to learn more about project.json  http://go.microsoft.com/fwlink/?LinkID=517074 */
    "webroot": "wwwroot",
    "version": "1.0.0-*",
    "dependencies": {
        "Microsoft.AspNet.Mvc": "6.0.0-*",
        "Microsoft.AspNet.StaticFiles": "1.0.0-*",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-*",
        "Microsoft.AspNet.Security.OAuthBearer": "1.0.0.0-*",
        "Castle.Windsor": "3.3.0",
        "Herolab.WebAPI.Config": "1.0.0-*",
        "Herolab.NowinHost": "1.0.0-*",
        "MySql.Data": "6.9.6"
    },
    "frameworks": {
        "aspnet50": {
            "frameworkAssemblies": {
                "System.Configuration": "4.0.0.0"
            }
        }
    },
    "commands": {
        "web": "Microsoft.AspNet.Hosting --server Herolab.NowinHost",
        "web-kestrel": "Microsoft.AspNet.Hosting --server Kestrel"
    },
    "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
    ],
    "bundleExclude": [
        "node_modules",
        "bower_components",
        "**.kproj",
        "**.user",
        "**.vspscc"
    ]
}
